{
  "name": "node-react-csv-reader",
  "version": "0.0.1",
  "description": "csv reader",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./build/index.js && npm run build --prefix client ",
    "install-all": "npm install && npm install --prefix client",
    "server": "nodemon",
    "client": "npm start --prefix client",
    "clean": "rm -rf build && mkdir build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build-babel": "babel  ./server  --out-dir ./build  --presets=@babel/env",
    "client-install": "cd client && npm install",
    "build": "npm run clean && npm run build-babel",
    "build-all": "NPM_CONFIG_PRODUCTION=false npm install && npm install --prefix client && npm run build && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oRLex/mern-instagram-project.git"
  },
  "author": "Ihor I",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oRLex/mern-instagram-project/issues"
  },
  "homepage": "https://github.com/oRLex/mern-instagram-project#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.12.13",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "babel-loader": "^8.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "babel-loader": "8.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "multer": "^1.4.2"
  }
}
